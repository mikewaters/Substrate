    rdfs:domain lifeos:Resource ;
    rdfs:range lifeos:Collection ;
    owl:inverseOf lifeos:has_resources ;
    skos:definition "Resource is part of an ongoing effort or activity on the life business" ;
    skos:inScheme lifeos:knowledge_management ;
    skos:note "A related resource in which the described resource is physically or logically included." .

lifeos:lifeos-ontology.owl.ttl a owl:Ontology ;
    rdfs:label "ontology" ;
    dcterms:license "https://creativecommons.org/publicdomain/zero/1.0/" ;
    skos:definition """A comprehensive ontology for modeling personal life management, including self-identity, work activities, and world objects.
""" .

lifeos:relation a owl:DatatypeProperty ;
    rdfs:label "relation" ;
    rdfs:range xsd:string ;
    skos:definition "unspecified relationship" ;
